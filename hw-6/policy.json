{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:*"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": "sqs:*",
            "Resource": ["arn:aws:sqs:*:*:*"]
        },
        {
            "Effect": "Allow",
            "Action": "sns:*",
            "Resource": ["arn:aws:sns:*:*:*"]
        }
    ]
}
